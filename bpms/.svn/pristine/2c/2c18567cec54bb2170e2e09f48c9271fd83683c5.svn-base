<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<div class="card">
		<div class="card-header" style="padding-top:55px;">
			<div class="card-title" style="padding-left: 15px">파일 관리</div>
		</div>
		<div class="card-body" style="padding-top: 50px">
			<div class="table-responsive">
				<table class="table table-bordered">
					<thead>
						<tr style="text-align: center">
							<th style="width: 35px;">#</th>
							<th>파일명</th>
							<th>등록일</th>
							<th>작성자</th>
							<th>버전</th>
							<th>체크</th>
						</tr>
					</thead>
					<tbody style="text-align: center;">
						<tr>
							<th scope="row">7</th>
							<td>요구사항 정의서</td>
							<td>2021-01-20</td>
							<td>이선엽</td>
							<td>1.0</td>
							<td><input type="checkbox" /></td>
						</tr>
						<tr>
							<th scope="row">6</th>
							<td>단위업무 정의서</td>
							<td>2020-04-28</td>
							<td>신광진</td>
							<td>1.0</td>
							<td><input type="checkbox" /></td>
						</tr>
						<tr>
							<th scope="row">5</th>
							<td>메뉴 구조도</td>
							<td>2021-01-02</td>
							<td>김근호</td>
							<td>1.0</td>
							<td><input type="checkbox" /></td>
						</tr>
						<tr>
							<th scope="row">4</th>
							<td>테이블 정의서</td>
							<td>2021-01-20</td>
							<td>전수빈</td>
							<td>1.0</td>
							<td><input type="checkbox" /></td>
						</tr>
						<tr>
							<th scope="row">3</th>
							<td>앞단의 신이 되는법</td>
							<td>2020-04-28</td>
							<td>임건</td>
							<td>1.0</td>
							<td><input type="checkbox" /></td>
						</tr>
						<tr>
							<th scope="row">2</th>
							<td>내가 곧 프론트다</td>
							<td>2021-01-02</td>
							<td>임건</td>
							<td>1.0</td>
							<td><input type="checkbox" /></td>
						</tr>
						<tr>
							<th scope="row">1</th>
							<td>앞단이 어려운가?</td>
							<td>2021-01-20</td>
							<td>임건</td>
							<td>1.0</td>
							<td><input type="checkbox" /></td>
						</tr>
					</tbody>
					<tfoot>
						<tr>
							<td colspan="6">
								<div>
									<button type="button" class="btn btn-danger" style="float:right;" >삭제</button>
									<button type="button" class="btn btn-primary" style="float:right; margin-right:10px">다운로드</button>
								</div>
							</td>
						</tr>
					</tfoot>
				</table>
			</div>
		</div>
	</div>
</body>
</html>