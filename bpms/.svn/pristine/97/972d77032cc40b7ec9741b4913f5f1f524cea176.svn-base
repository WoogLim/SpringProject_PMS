}

.nav-active::after {
    position: absolute;
    content: " ";
    bottom: 0;
    left: 3.5%;
    width: 93%;
    display: block;
    background-color: #6861ce;
    height: 4px;
}

.usernav-active::after {
    position: absolute;
    content: " ";
    bottom: 0;
    left: 3.5%;
    width: 93%;
    display: block;
    background-color: #1572e8;
    height: 4px;
}

.main-navbtn i {
