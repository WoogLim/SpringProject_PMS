}

.setting-section {
    padding: 60px 0;
    flex-flow: column nowrap;
    align-items: center;
    width: 50%;
    margin: 0 auto;
    box-sizing: border-box;
}

.unset {
    all: unset;
    display: inline-block;
}

.setting-head {
    width: 100%;
}

.setting-head .dropdown-item {
    font-size: 1rem;
}

.more-select {
    border-radius: 5px;
    padding: 5px;
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 5;
    font-size: 24px;
    cursor: pointer;
}

.more-select:hover {
    background: #eeeeee;
}

.project-image {
    padding-top: 30px;
    margin: 0 auto;
}

.project-image img {
