    justify-content: center;
    align-items: center;
    font-weight: 600;
    font-size: 20px;
}

.custompopupbtn-group{
    flex:1;
    display: flex;
    width:100%;
}

.custompopupbtn-group button{
    all:unset;
    flex:1;
    display: flex;
    justify-content: center;
    align-items: center;
    color:#ffffff;
    font-weight: 600;
    font-size: 18px;
    cursor: pointer;
}

.custompopupbtn-group .custombtn-cancel{
    background: #fa5e8d;
    border-radius: 0 0 8px 8px;
}   

