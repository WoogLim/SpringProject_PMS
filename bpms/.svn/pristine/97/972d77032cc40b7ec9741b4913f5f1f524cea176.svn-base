p {
    margin: 0;
}

a {
    text-decoration: none !important;
}

body {
    background: #ffffff;
}

ul {
    list-style: none !important;
}

.clearfix::after {
    content: "";
    clear: both;
    display: block;
}

.main-navitems ul {
    padding: 0;
    list-style: none;
}

.float--left {
    float: left;
}

.float--right {
    float: right;
}

.header__work {
    margin-top: 30px;
}

.header__work h1 {
    font-size: 20px;
}

.project-search {
    margin-right: 20px;
}

.project-filter,
.history-filter {
    display: flex;
    align-items: center;
}

/* 검색 양식 */
.bpms-searchgroup {
    position: relative;
}

.bpms-searchgroup i {
    position: absolute;
    top: 8.5px;
    right: 15px;
    color: #eeeeee;
}

.bpms-search {
    all: unset;
    background: #ffffff;
    height: 30px;
    border-radius: 25px;
    box-shadow: 0 0 4px rgba(0, 0, 0, 0.1);
    border: 1px solid #eeeeee;
    padding: 0 10px;
}

.bpms-search:focus {
    border: 1px solid #1572E8;
    background: #ffffff;
}

.bpms-search:focus .bpms-searchgroup i {
    color: #1572E8;
}

/* 버튼 css */
.btn-bpms {
    -webkit-align-items: baseline;
    -webkit-box-align: baseline;
    -ms-flex-align: baseline;
    align-items: baseline;
    border-width: 0;
    box-sizing: border-box;
    display: inline-flex;
    font-size: 13px;
    font-style: normal;
    font-weight: 500;
    max-width: 100%;
    outline: none !important;
    text-align: center;
    -webkit-text-decoration: none;
    text-decoration: none;
    white-space: nowrap;
    background: #0052CC;
    border-radius: 3px;
    box-shadow: 0 0 0 2px inherit;
    color: #FFFFFF !important;
    cursor: pointer;
    height: 2.15em;
    line-height: 1.2em;
    padding: 6px 12px;
    -webkit-transition: background 0.1s ease-out, box-shadow 0.15s cubic-bezier(0.47, 0.03, 0.49, 1.38);
    transition: background 0.1s ease-out, box-shadow 0.15s cubic-bezier(0.47, 0.03, 0.49, 1.38);
    -webkit-transition-duration: 0.1s, 0.15s;
    transition-duration: 0.1s, 0.15s;
    vertical-align: middle;
    width: auto;
}

.btn-bpms:hover {
    background: #0066ff;
}

/* 헤더 인풋 박스 */
.input--text {
    width: 160px;
    height: 28px;
    margin-top: 2px;
    padding: 5px 20px;
    border-radius: 5px;
    box-sizing: border-box;
    outline: none;
    font-size: 13px;
    line-height: 20px;
    border: 2px solid rgb(223, 225, 230);
    box-shadow: none;
    background-color: rgb(255, 255, 255);
    color: rgb(107, 119, 140);
}

.input--text:focus {
    border-color: #0066ff;
}

/*  프로젝트 섹션 */
.page-inner {
    min-width: 756px;
}

.item-section {
    position: relative;
    padding: 0 40px;
}

.work-container {
    display: flex;
    -webkit-flex: 1 1 100%;
    -ms-flex: 1 1 100%;
    flex: 1 1 100%;
    flex-flow: column nowrap;
}

.header-work h1 {
    font-size: 22px;
}

.search-project {
    position: relative;
    width: 180px;
}

.search-project .input--text {
    width: 100%;
    background: #fcfcfc;
}

.search-project .input--text:hover {
    width: 100%;
    background: #f3f3f3;
    transition: .3s;
}

.search-project .input--text:focus {
    width: 100%;
    background: #ffffff;
    transition: .3s;
}

.search-project .fa-search {
    position: absolute;
    right: 8px;
    top: 10px;
    font-size: 12px;
    color: #c7c7c7;
}

.project-title,
.history-title {
    margin-bottom: 10px !important;
    padding: 10px !important;
    font-size: 15px;
    border-bottom: 1px solid #f9f9f9;
}

.project-title li:first-child {
    padding-left: 10px !important;
}

.project-title li:nth-child(2) {
    padding-left: 5px !important;
}

.project-title,
.project-list,
.history-title,
history-list {
    padding: 0 !important;
    display: flex;
}

.history-item {
    padding: 10px !important;
    border-radius: 5px;
    background: #f9f9f9;
    height: 60px !important;
}

.history-item:not(:last-child) {
    margin-bottom: 10px !important;
}

.history-role {
    margin-bottom: 20px !important;
}

.history-guide {
    color: #ffffff;
    font-weight: 500;
    font-size: 12px;
    background: #7b75cc;
    padding: 5px 10px;
    border-radius: 7.5px;
}

.project-authentication {
    display: flex;
    flex: 1;
    align-items: center;
    position: relative;
}

.project-detailviewbtn {
    all: unset;
    display: flex;
    cursor: pointer;
    align-items: center;
    position: relative;
}

.project-detailviewbtn span {
    font-size: 14px;
    font-family: 200;
}

.project-detailviewbtn i {
    font-size: 16px;
    padding-top: 1px;
    margin-right: 3px;
}

.project-detailviewbtn:hover {
    color: #1572e8;
    transition: .3ms;
}

.project-authentication .avatar-sm {
    width: 28px;
    height: 28px;
    display: flex;
    align-items: center;
}

.project-authentication .project-leader {
    margin-left: 10px;
    font-size: 14px;
}

