    padding: 0;
}

.pagination-container {
    margin-top: 20px;
    display: flex;
    justify-content: flex-end;
}

.page-link span {
    display: flex;
}

.player-card {
    max-height: 600px;
    height: 100%;
}

.notice-card {
    max-height: 600px;
    height: 100%;
}

.history-item {
    display: flex;
}

.project-card {
    max-height: 600px;
}

.projectapply-card {
    min-height: 400px;
    max-height: 600px;
}

.project-table {
    max-height: 400px;
    overflow: scroll;
}

.history-body {
    padding: 0 40px;
}

.card-body,
.project-table {
    overflow: scroll;
    padding-left: 40px;
}

.card-body::-webkit-scrollbar {
    width: 20px;
    scrollbar-width: thin;
    scrollbar-color: rgb(174, 200, 255);
}

.card-body::-webkit-scrollbar-track {
    background-color: transparent;
}

.card-body::-webkit-scrollbar-thumb {
    border-radius: 10px;
    background-color: rgb(174, 200, 255);
}

.card-body:-webkit-scrollbar-button {
    width: 0;
    height: 0;
}

.history-card .card-body {
    padding-top: 0px !important;
}

.card-body .project-rules {
    min-height: 60px;
    display: flex;
    align-items: center;
}

.card-body ul {
    padding: 0px 10px;
}

.history-searchfilter {
    width: 100%;
    padding: 0 10px;
    font-weight: 400;
    display: flex;
    align-items: center;
    justify-content: space-between !important;
}

.notice-item,
.member-item {
    padding: 10px;
    border-radius: 5px;
    display: flex;
    align-items: center;
    background: #f9f9f9;
    cursor: pointer;
}

.selected-title {
    font-weight: 600;
    margin-right: 5px;
    color: #0052cc;
}

.end-alert {
    margin-left: 10px;
}

.start-alert::after,
.end-alert::after {
    content: ":";
}

.selected-startdatetime,
.selected-enddatetime {
    font-weight: 500;
}

.selected-startdatetime::after {
    content: "~";
