/* 개인정보 설정 */

.mysetting-container{
    padding: 1.5rem;
    max-width: 764px;
    margin: 0 auto;
}

.mysetting-title{
    margin:0;
    padding:0;
    margin-bottom: 10px;
}

.myprofileinfo-container{
    display: flex;
    flex-flow: column nowrap;
}

.profileimg-card{
    display: flex;
    flex-flow: column nowrap;
}

.profileimg-card{
    height:210px;
    margin-bottom: 20px;
}

.mysection-title{
    font-size: 18px;
}

.profileimg-card{
    box-shadow: 0 0 10px rgba(0, 0, 0, .1);
}

.image-preview{
    height: 130px;
    background-image: url(../img/bg-404.jpeg);
    background-repeat: no-repeat;
    background-size: cover;
    position: relative;
}

.image-upload{
    position: relative;
    width: 140px;
    height: 140px;
    display: flex;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    border-radius: 100%;
    top:40px;
    left:60px;
    box-shadow: 0 0 10px rgba(0, 0, 0, .1);
    border:5px solid #ffffff;
    background: #ffffff;
    cursor: pointer;
}

.image-upload::after {
    content:"프로필 변경";
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    text-align: center;
    padding-top: 50px;
    width: 100%;
    height: 100%;
    border-radius: 100%;
    background: #1571e888;
    color:#ffffff;
    font-size: 18px;
    font-weight: 500;
    display: none;
}

.image-upload:hover::after{
    display: block;
}

.image-upload img {
    width:100%;
    height:100%;
    border-radius: 100%;
    object-fit: contain;
}

.file-upload-input {
    position: absolute;
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    cursor: pointer;
    z-index: 10;
}

.profileuser-name{
    color:#ffffff;
    position: absolute;
    left: 210px;
    top:95px;
    font-size: 28px;
    font-weight: 600;
    text-shadow: 0 0 10px rgba(0, 0, 0, .4);
}

.userprofile-info{
    display: flex;
    justify-content: flex-end;
    align-items: flex-end;
    height:100%;
    padding:20px;
}

.userprofile-email i{
    padding-top: 5px;
    margin-right: 5px;
    color:#1572e8;
}

.userprofile-email{
    display: flex;
    align-items: center;
    font-size: 16px;
}

.passchange-card{
    box-shadow: 0 0 10px rgba(0, 0, 0, .1);
    padding:15px;
    display: flex;
    flex-flow: column nowrap;
}

.passcard-inner {
    display: flex;
    flex-flow: column nowrap;
}

.passinput-checker{
    display: flex;
    align-items: center;
    font-size: 16px;
    font-weight: 500;
    color:#333333;
}

.passinput-checker::before{
    content:"*";
    padding-top: 10px;
    margin-right: 2.5px;
    font-size: 16px;
    color:rgb(255, 99, 99);
}

.passinput-change{
    margin-top: 10px;
}

.currentpass-input,
.changepass-input {
    background-color: rgb(250, 251, 252);
    border-color: rgb(223, 225, 230);
    border-radius: 3px;
    border-width: 2px;
    border-style: solid;
    box-sizing: border-box;
    color: rgb(9, 30, 66);
    font-size: 14px;
    max-width: 100%;
    overflow: hidden;
    overflow-wrap: break-word;transition: background-color 0.2s ease-in-out 0s, border-color 0.2s ease-in-out 0s;
    padding:0 5px;
    height:38px;
}

.currentpass-input:hover,
.changepass-input:hover {
    background-color: rgb(223, 225, 230);
}

.currentpass-input:focus,
.changepass-input:focus{
    border-color: #1572e8;
    background: #ffffff;
}

.passchange-submit{
    all: unset;
    margin-top: 20px;
    background:#f9f9f9;
    color:#aeaeae;
    padding:7px 8px;
    width: 100px;
    display: flex;
    justify-content: center;
}

.passchange-error{
    font-size: 14px;
    display: none;
    font-weight:500;
}

#currentpass-error[data-msgtype="error"]{
    color:#ff6b6b;
}

#newpass-error[data-msgtype="error"]{
    color:#ff6b6b;
}

#currentpass-error[data-msgtype="success"]{
    color:#417cfd;
}

#newpass-error[data-msgtype="success"]{
    color:#417cfd;
}

