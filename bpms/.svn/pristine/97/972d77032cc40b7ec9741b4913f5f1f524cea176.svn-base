.project-title li,
.project-list li,
.history-title li,
.history-list li {
    padding: 0;
    display: flex;
    align-items: center;
}

.project-list {
    padding: 10px !important;
    background: #f9f9f9;
    border-radius: 5px;
    font-size: 15px;
    min-height: 50px;
}

.history-list {
    min-height: 40px;
    cursor: pointer;
}

.project-list:hover,
.history-list:hover {
    background: #dbe3ff;
    box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.1);
}

.project-list:not(:last-child),
.history-list:not(:last-child) {
