}

.userlogout-btn {
    margin-left: 20px !important;
    all: unset;
    color: #1572e8;
    font-weight: 600;
    cursor: pointer;
    border-bottom: 2px solid #ffffff;
}

.userlogout-btn:hover {
    border-bottom: 2px solid #1572e8;
}

.history-box {
    display: none;
}

.projectperson-group .avatar {
    width: 40px;
    height: 40px;
}

.notice-item:hover,
.member-item:hover,
.history-item:hover,
.request-item:hover {
    background: #dbe3ff;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
}

.noticeitem-info,
.member-info {
    display: flex;
    flex-flow: column;
    justify-content: space-between;
    height: 48px;
    padding-left: 10px;
    font-size: 14px;
    font-weight: 500;
}

.noticeitem-datatime,
.member-createtime {
    height: 48px;
    display: flex;
    flex: 1;
    justify-content: flex-end;
    align-items: flex-end;
