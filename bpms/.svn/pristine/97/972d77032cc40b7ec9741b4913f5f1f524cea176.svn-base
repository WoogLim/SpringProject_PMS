    width: 140px;
    background: #11c0e4;
    padding: 10px;
}

.proimg-btn {
    margin: 20px 0 20px 0;
    font-size: 1.2rem;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 7px 14px;
    background: rgba(211, 211, 211, 0.08);
}

.proimg-btn:hover {
    background: rgb(238, 238, 238);
}

.setting-input {
    width: 100%;
    border: 1px solid #eeeeee;
    background: rgba(211, 211, 211, 0.08);
    padding: 12px 8px;
    display: block;
    box-sizing: border-box;
    font-size: 1rem;
}

.setting-input:focus {
    border: 1px solid #0066ff;
    background: #ffffff;
}

.setting-title {
    margin-top: 40px;
}

.project-settingform {
    max-width: 450px;
    width: 75%;
}

.project-path {
    width: 100%;
}

.btn-submit {
    margin-top: 30px;
    background: #0066ff;
    padding: 10px 15px;
    color: #ffffff;
    font-size: 1rem;
}

.user-select {
    display: block;
    width: 100%;
}

.user-select .btn {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 60px;
}

.user-select .dropdown-menu {
    width: 100%;
}

.user-select .dropdown-item {
    padding: 20px;
    font-size: 1rem;
    text-transform: uppercase;
}

.project-settingform .btn-submit {
    margin-top: 60px;
}

.project-summarize {
    flex-flow: column nowrap;
}

.project-summarize h1 {
    border-bottom: 1px solid rgb(238, 238, 238);
