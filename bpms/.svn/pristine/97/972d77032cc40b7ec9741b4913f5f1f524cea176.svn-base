    justify-content: flex-end !important;
}

.history-date span {
    margin-right: 10px;
}

.project-titlename {
    font-size: 15px;
    color: #2c2c2c;
    font-weight: 600;
    cursor: pointer;
}

.project-titlename:hover {
    text-decoration: underline;
    color: #1572e8;
    transition: .3ms;
}

.history-status,
.history-manager,
.history-priority,
.history-prograss {
    display: flex;
    justify-content: center !important;
}

.importantproject {
    color: #ffee00;
}

.project-icon {
    background-image: url(../img/flaticon/clipboard.svg);
