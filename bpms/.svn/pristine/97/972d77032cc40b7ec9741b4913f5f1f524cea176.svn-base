}

.history-status {
    margin-right: 7.5px;
}

.history-status .done {
    font-size: 30px;
    color: #1572E8;
}

.history-simpleinfo {
    display: flex;
    flex-flow: column;
}

.history-worktitle {
    display: flex;
}

.history-worktitle .history-name {
    font-size: 14px;
    font-weight: 500;
}

.history-name::after {
    content: "·";
    margin: 0px 2.5px;
}

.history-type::before {
    content: "·";
    margin: 0px 2.5px;
}

.history-path {
    display: flex;
    font-size: 12px;
    font-weight: 500;
    color: #6b778c;
}

.history-code::after,
.history-manager::after {
    content: "·";
    margin: 0px 2.5px;
}

.history-content {
    display: block;
    font-size: 13px;
    overflow: hidden;
    height: 40px;
}

.history-detailinfo {
    padding: 0px;
}

.history-shortinfo {
    display: flex;
    align-items: center;
    padding: 0px;
}

.history-memberavatar {
    display: flex;
    justify-content: flex-end;
}

.history-item {
    cursor: pointer;
}

.request-title {
    font-size: 16px;
    font-weight: 500;
    border-bottom: 1px solid #f9f9f9;
    padding-bottom: 5px !important;
}

.request-titlename {
    display: flex;
    align-items: center !important;
}

.request-totalnumber {
    color: #1572E8;
    font-weight: 900;
    margin-left: 5px;
    font-size: 16px;
}

.request-item {
    background: #F9F9F9;
    display: flex;
    width: 100%;
    height: 60px;
    border-radius: 5px;
    padding: 10px !important;
    cursor: pointer;
}

.request-totalnumber::before {
    content: "+";
}

.request-item:not(:last-child) {
    margin-bottom: 10px !important;
}

.request-icon {
    display: flex;
    align-items: center;
    padding: 0px;
}

.request-procontent {
    padding: 0px;
    height: 40px;
    overflow: hidden;
}

.request-icon {
    overflow: hidden;
}

.request-icon i {
    font-size: 28px;
    color: #1572e8;
}

.request-protitle {
    display: flex;
    flex-flow: column;
    margin-left: 7.5px;
}

.request-proname {
    font-weight: 500;
}

.request-manage {
    color: #6b778c;
    font-weight: 500;
    font-size: 12px;
}

.pro-member {
    position: relative;
}

.person-name {
    display: none;
    white-space: nowrap !important;
    background: #525252;
    color: #ffffff;
    min-width: 60px !important;
    width: auto !important;
    text-align: center;
    font-size: 12px;
    border-radius: 15px;
    position: absolute;
    right: -10px;
    z-index: 10;
}

.request-manager::before {
    content: "·";
}

.main-navigation {
    padding: 10px !important;
}

.main-navbtn,
.usermain-navbtn {
    all: unset;
    font-size: 18px;
    cursor: pointer;
    height: 50px;
    padding: 0 10px;
    font-size: 18px;
    font-weight: 600;
}

.main-navbtn:hover {
