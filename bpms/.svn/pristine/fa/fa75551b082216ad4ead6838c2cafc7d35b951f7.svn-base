$(function(){
   let url = (window.location.href).split("/");
   
   console.log(url[5]);
   
   if(url.includes('mypage')){
      $('.nav-myprofile').addClass('active');
   }
   else if(url.includes('mymessage')){
      $('.nav-mymessage').addClass('active');
   }
   else if(url.includes('kanbanView.do')){
      $('.nav-sticker').addClass('active');
   }
   else if(url.includes('mySetting')){
      $('.nav-mysecurity').addClass('active');
   }else{
	   return;
   }
})