<!-- Fonts and icons -->
<script src="${pageContext.request.contextPath }/assets/js/plugin/webfont/webfont.min.js"></script>
<script>
WebFont.load({
    custom: {
        "families": ["Flaticon", "Font Awesome 5 Solid", "Font Awesome 5 Regular",
